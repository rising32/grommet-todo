{
  "name": "grommet-sample",
  "version": "0.2.0",
  "authors": [
    "Alan Souza",
    "Bryan Jacquot",
    "Chris Carlozzi",
    "Eric Soderberg"
  ],
  "homepage": "https://grommet.github.io",
  "bugs": "https://github.com/grommet/grommet-sample/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/grommet/grommet-sample.git"
  },
  "dependencies": {
    "grommet": "https://github.com/grommet/grommet/tarball/stable",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-parallel": "^0.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "^0.9.0",
    "grommet-cli": "^3.0.0",
    "node-sass": "^4.1.1",
    "pre-commit": "^1.2.2",
    "react-dev-utils": "^0.4.2",
    "react-test-renderer": "^15.4.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "webpack": "^2.2.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/templates/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|templates|node_modules)[/\\\\]"
    ]
  },
  "scripts": {
    "test": "grommet check",
    "dev": "cross-env NODE_ENV=development grommet pack",
    "dist": "cross-env NODE_ENV=production grommet pack"
  },
  "pre-commit": [
    "test"
  ]
}
